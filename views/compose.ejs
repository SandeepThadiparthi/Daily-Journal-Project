<h1>Compose</h1>
<form  action="/compose" method="post">
    <div class="form-group">
    <label for="title">Title</label>
    <input class="mb-3 form-control" type="text" id="title" name="postTitle" required autofocus>
    <label for="exampleFormControlTextarea1">Post</label>
    <textarea class="form-control" name="postBody" id="exampleFormControlTextarea1" rows="8" cols="10"></textarea>
</div>

    <button class="btn btn-primary btn-bg-blue" type="submit">Publish</button>
</form>

